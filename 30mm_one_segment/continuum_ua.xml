    <site size="0.002" rgba="0.2 0.8 1 1"/>
    <tendon stiffness="0" damping="0" frictionloss="0.05"/>
    <position kp="5000" ctrlrange="-0.05 0.05"/>
  </default>

  <!-- segment spacing d -->
  <!-- d = 0.012 m per vertebra interval; edit as needed -->

